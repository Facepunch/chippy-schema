{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "/chippy-schema/schemas/gamemodedata.json",
  "title": "SpaceUsurper.GameModeData",
  "description": "",
  "type": "object",
  "properties": {
    "properties": {
      "type": "object",
      "patternProperties": {
        ".*": {
          "$ref": "/chippy-schema/schemas/propertydata.json"
        }
      }
    },
    "scoreFunction": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "$ref": "/chippy-schema/schemas/iscriptfunc.json"
        }
      ]
    },
    "progressFunction": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "$ref": "/chippy-schema/schemas/iscriptfunc.json"
        }
      ]
    },
    "showTimer": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "$ref": "/chippy-schema/schemas/iscriptfunc.json"
        }
      ]
    },
    "showProgress": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "$ref": "/chippy-schema/schemas/iscriptfunc.json"
        }
      ]
    },
    "onSetup": {
      "$ref": "/chippy-schema/schemas/actionlist.json"
    },
    "onUpdate": {
      "$ref": "/chippy-schema/schemas/actionlist.json"
    },
    "onPlayerDeath": {
      "$ref": "/chippy-schema/schemas/actionlist.json"
    },
    "onFormDestroy": {
      "$ref": "/chippy-schema/schemas/actionlist.json"
    },
    "onDefeat": {
      "$ref": "/chippy-schema/schemas/actionlist.json"
    },
    "onVictory": {
      "$ref": "/chippy-schema/schemas/actionlist.json"
    },
    "#include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "precacheTextures": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "/chippy-schema/schemas/datapath.json"
          }
        ]
      }
    }
  }
}